{
	"extends": ["next/core-web-vitals", "prettier"],
	"plugins": ["prettier", "no-only-tests"],
	"rules": {
		"prettier/prettier": "error",
		"no-console": "warn",
		"no-only-tests/no-only-tests": "error",
		"import/order": [
			"error",
			{
				"groups": [
					"builtin",
					"external",
					"internal",
					"parent",
					"sibling",
					"index"
				],
				"newlines-between": "always",
				"alphabetize": {
					"order": "asc",
					"caseInsensitive": true
				}
			}
		]
	}
}
