{
	"name": "forvoyez",
	"version": "1.4.3",
	"private": false,
	"scripts": {
		"dev": "next dev --turbopack -H 0.0.0.0",
		"build": "next build",
		"start": "next start",
		"launch": "prisma generate && prisma migrate deploy && next start",
		"prettier": "prettier --write .",
		"precommit": "lint-staged",
		"prepare": "husky",
		"prisma": "prisma generate && prisma migrate dev",
		"prisma:migrate": "prisma migrate dev",
		"prisma:generate": "prisma generate",
		"prisma:seed": "prisma db seed",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "vitest",
		"test:ui": "vitest --ui --coverage --coverage.enabled=true",
		"test:ci": "vitest --coverage --coverage.enabled=true",
		"test:e2e": "playwright test",
		"test:e2e:general": "playwright test tests/e2e/general && playwright test tests/e2e/subscribed/app-subscribed-acc",
		"test:e2e:sub": "playwright test tests/e2e/subscribed/premium"
	},
	"lint-staged": {
		"*.js": [
			"prettier --write",
			"eslint --fix"
		],
		"*.json": "prettier --write",
		"*.css": "prettier --write"
	},
	"prisma": {
		"seed": "node prisma/seed.js"
	},
